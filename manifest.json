{
  "name": "Quick Translate",
  "description": "Translate an English Paragraph to Vietnamese using Bing API",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Quick Translate",
    "default_popup": "popup.html",
    "default_icon": "bing.png"
  },
  "manifest_version": 2
}