{
  "name": "Page Redder",
  "description": "Make the current page red",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "http://www.evangelsenglish.com/*",
    "https://www.evangelsenglish.com/*"
  ],
  "browser_action": {
    "default_title": "Make this page red",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_scripts": [ {
    "js": [ "jquery.min.js", "background.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}