{
  "name": "Quick Translate",
  "description": "Translate an English Paragraph to Vietnamese using Bing API",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Make this page red",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_scripts": [ {
    "js": [ "jquery.min.js", "background.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}